{% if admin.isGranted('EDIT', object) and admin.hasRoute('edit') %}
    {% set url =  path('cocorico_user_profile_show', { 'id': object.c4Id }) %}

    <a href="{{ url }}" target="_blank" class="btn btn-sm btn-default edit_link"
       title="Profil">
        <i class="glyphicon glyphicon-list"> </i>
        Afficher Utilisateur
    </a>
{% endif %}
