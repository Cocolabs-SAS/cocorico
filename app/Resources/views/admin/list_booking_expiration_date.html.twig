{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {{ object.newBookingAt ?
    object.getExpirationDate(
        field_description.options.bookingExpirationDelay,
        field_description.options.bookingAcceptationDelay
